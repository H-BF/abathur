{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "./build/client.js",
  "scripts": {
    "build": "tsc",
    "rebuild": "rimraf ./build && tsc",
    "start:prod": "node ./build/client.js",
    "start:client": "rimraf ./build && tsc && node ./build/client.js",
    "proto:hbf": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=./gRPC ./gRPC/HBFTest.proto",
    "proto:reporter": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=./gRPC ./gRPC/reporter.proto",
    "proto:control": "proto-loader-gen-types --grpcLib=@grpc/grpc-js --outDir=./gRPC ./gRPC/control.proto"
  },
  "author": "Ari Insomnia",
  "license": "ISC",
  "devDependencies": {
    "tslib": "^2.5.3",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.8.16",
    "@grpc/proto-loader": "^0.7.7",
    "http-proxy-agent": "^7.0.0",
    "rimraf": "^5.0.1"
  }
}
